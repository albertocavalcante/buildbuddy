{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "sourceMap": false,
    "declaration": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "target": "ES2016",
    "lib": ["es2017", "es2019", "dom", "dom.iterable"]
  }
}
